{
  "name": "vscode-webview-react",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.23.0"
  },
  "publisher": "rebornix",
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./build/ext-src/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "remix-light",
          "title": "remix-light",
          "icon": "./logo.svg"
        }
      ]
    },
    "views": {
      "remix-light": [
        {
          "type": "webview",
          "id": "remix-light-view",
          "name": "Panel"
        }
      ]
    }
  },
  "dependencies": {
    "@ethereumjs/util": "^8.0.5",
    "@remix-project/remix-simulator": "^0.2.29",
    "@remix-project/remix-solidity": "^0.5.12",
    "@remixproject/engine": "^0.3.33",
    "@remixproject/engine-web": "^0.3.33",
    "@remixproject/plugin": "^0.3.37",
    "@remixproject/plugin-vscode": "^0.3.37",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.34",
    "@types/react": "^18.2.7",
    "@types/react-dom": "^18.2.4",
    "axios": "^0.20.0",
    "constants-browserify": "^1.0.0",
    "copy-to-clipboard": "^3.3.3",
    "crypto-js": "^4.1.1",
    "file-saver": "^2.0.5",
    "gm-crypto": "^0.1.8",
    "jsbn": "^1.1.0",
    "react": "^17.0.2",
    "react-app-rewired-esbuild": "^0.0.11",
    "react-bootstrap": "^1.6.4",
    "react-dom": "^17.0.2",
    "react-intl": "^6.4.4",
    "react-scripts": "5.0.1",
    "sm-crypto": "^0.3.8",
    "solc": "^0.7.4",
    "terser": "^5.15.0",
    "typescript": "^4.9.5",
    "vm-browserify": "^1.1.2",
    "web-vitals": "^2.1.4",
    "web3": "^1.10.0"
  },
  "resolutions": {},
  "scripts": {
    "vscode:prepublish": "node ./scripts/build-non-split.js && tsc -p tsconfig.extension.json",
    "start": "react-app-rewired start",
    "build": "react-app-rewired build && tsc -p tsconfig.extension.json",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-app-rewired eject"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.1.1",
    "@types/jest": "^23.3.13",
    "@types/jsbn": "^1.2.30",
    "@types/node": "^10.1.2",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "@types/sm-crypto": "^0.3.0",
    "@types/vscode": "^1.34.0",
    "assert": "^2.0.0",
    "browserify-zlib": "^0.2.0",
    "crypto-browserify": "^3.12.0",
    "https-browserify": "^1.0.0",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "react-app-rewired": "^2.2.1",
    "react-scripts": "^5.0.1",
    "rewire": "^6.0.0",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "typescript": "^3.3.1",
    "url": "^0.11.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
