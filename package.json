{
  "name": "vscode-webview-react",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.23.0"
  },
  "publisher": "rebornix",
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./build/ext-src/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "remix-light",
          "title": "remix-light",
          "icon": "./logo.svg"
        }
      ]
    },
    "views": {
      "remix-light": [
        {
          "type": "webview",
          "id": "remix-light-view",
          "name": "Panel"
        }
      ]
    }
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "terser": "^5.15.0"
  },
  "resolutions": {
  },
  "scripts": {
    "vscode:prepublish": "node ./scripts/build-non-split.js && tsc -p tsconfig.extension.json",
    "start": "react-app-rewired start",
    "build": "node ./scripts/build-non-split.js && tsc -p tsconfig.extension.json",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-app-rewired eject"
  },
  "devDependencies": {
    "@types/jest": "^23.3.13",
    "@types/node": "^10.1.2",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "@types/vscode": "^1.34.0",
    "react-scripts": "^5.0.1",
    "react-app-rewired": "^2.2.1",
    "rewire": "^6.0.0",
    "typescript": "^3.3.1"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
